<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--
    See also value-v11 style file
    -->
    <style name="MyTheme" parent="MaterialDrawerTheme">

        <item name="colorAccent">@color/pressed_color</item>
        <item name="colorPrimary">@color/action_bar</item>
        <item name="colorPrimaryDark">@color/action_bar</item>

        <item name="iconSelected">@drawable/rating_important</item>
        <item name="iconTags">@drawable/ic_action_done</item>
        <item name="iconSearch">@drawable/action_search</item>
        <item name="iconSort">@drawable/collections_sort_by_size</item>
        <item name="iconRefresh">@drawable/navigation_refresh_d</item>
        <item name="iconLock">@drawable/ic_https_white_18dp</item>
        <item name="colorBg">@color/BLACK</item>
        <item name="colorText">@color/WHITE</item>


    </style>
    <style name="MyThemeLight" parent="MaterialDrawerTheme.Light">

        <item name="colorAccent">@color/pressed_color</item>

        <item name="colorPrimary">@color/action_bar_light</item>
        <item name="colorPrimaryDark">@color/action_bar_light</item>

        <item name="iconSelected">@drawable/rating_important_l</item>
        <item name="iconTags">@drawable/navigation_accept</item>
        <item name="iconSearch">@drawable/action_search_l</item>
        <item name="iconSort">@drawable/collections_sort_by_size_l</item>
        <item name="iconRefresh">@drawable/navigation_refresh</item>
        <item name="iconLock">@drawable/ic_https_grey600_18dp</item>
        <item name="colorBg">@color/WHITE</item>
        <item name="colorText">@color/BLACK</item>


    </style>



</resources>